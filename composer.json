{
    "name": "maxa-ondrej/nette-graphql",
    "description": "Nette Framework GraphQLite Integration Extension",
    "license": "MIT",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Maxa\\Ondrej\\Nette\\GraphQL\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/app/"
        }
    },
    "require": {
        "php": "^8.0",
        "contributte/cache": "^v0.6.0",
        "contributte/di": "^0.5.0",
        "psr/cache": "^1.0",
        "psr/container": "^1.0",
        "thecodingmachine/graphqlite": "^v5.0.0"
    },
    "require-dev": {
        "ninjify/nunjuck": "^0.4",
        "contributte/dev": "^0.3",
        "mockery/mockery": "^1.3.0",
        "nelmio/alice": "^3.5.8",
        "phpstan/phpstan": "^1.0",
        "phpstan/phpstan-nette": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "orisai/coding-standard": "^2.2",
        "jetbrains/phpstorm-attributes": "^1.0",
        "nette/application": "^3.0",
        "nette/bootstrap": "^3.0",
        "tracy/tracy": "^2.7"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "platform": {
            "php": "8.0"
        },
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "extra": {
        "enable-patching": true
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}